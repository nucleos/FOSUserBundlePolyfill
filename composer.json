{
    "name": "nucleos/fos-user-bundle-polyfill",
    "type": "symfony-bundle",
    "description": "Polyfill to provide base FOSUserBundle model interfaces for legacy applications",
    "keywords": [
        "user management",
        "symfony",
        "fosuserbundle",
        "userbundle"
    ],
    "homepage": "https://core23.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "require": {
        "php": "^7.3",
        "nucleos/user-bundle": "^1.0",
        "nucleos/profile-bundle": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3",
        "ergebnis/composer-normalize": "^2.0.1"
    },
    "replace": {
        "friendsofsymfony/user-bundle": "2.999"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nucleos\\UserBundlePolyfill\\Tests\\": "tests/"
        },
        "files": [
            "src/polyfill.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    }
}
